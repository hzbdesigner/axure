@charset "UTF-8";
/* CSS Document */
body{font-family:'Microsoft YaHei UI', 'Segoe UI', Arial, Verdana, sans-serif;
text-decoration: none;
}

.main_header{width:980px; margin:0 auto; }
.main_logo{ float:left; margin:20px 0 0 0;}
.main_hlink{ float:right; width:240px; margin-right:20px;margin:20px 0 0 0;}
.main_hlink ul li{float:left; margin-right:5px; }
.main_hlink ul li a{color:#3b3; font-size:16px;}

.main_nav,.main_nav_cur,.main_quickLink ul li,.main_link ul li,.main_link_content ul li,.main_link_content ul .link_content4 .box1,.main_link_content ul .link_content4 .box2,.main_link_content ul .link_content4 .box3,.main_link_content ul .link_content4 .box4,.main_tool,.main_map_wrap,.main_hospital_wrap{
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	behavior: url(/pie/PIE.htc);
	}

.main_nav { 
	margin:20px 0;
	
	/*border: 1px solid #696;*/
	line-height:56px;
	text-align: center; 
	width: 980px;
	height:56px;
	
	background: #97d543;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#97d543), to(#8cc63e));
	background: -webkit-linear-gradient(#97d543, #8cc63e);
	background: -moz-linear-gradient(#97d543, #8cc63e);
	background: -ms-linear-gradient(#97d543, #8cc63e);
	background: -o-linear-gradient(#97d543, #8cc63e);
	background: linear-gradient(#97d543, #8cc63e);
	-pie-background: linear-gradient(#97d543, #8cc63e);
	
}
.main_nav ul li{ float:left;}
.main_nav ul li a{
	display:block;
	float:left;
	
	margin-left:40px;
	color:#fff; 
	font-size:18px;
} 
.main_nav_cur{ 
	border: 2px solid #d0ff90;
	text-align: center; 
	
	display:block;
	padding:2px 10px;
	height:32px;
	line-height:32px;
	margin-top:8px;
	

}
.main_ad{ width:980px; margin:20px auto 0 auto;}
.main_ad_pic{ width:808px; height:234px; overflow:hidden; float:right; background:#9C0;}
.main_quickLink{ float:left;}
.main_quickLink ul li{
	position:relative;
	width:158px;
	height:52px;
	line-height:52px;
	margin-bottom:6px;
	font-size:18px;
	color:#FFF;
	text-align:center;
	
	/*border: 1px solid #696;*/
	background: #97d543;
	
}
.main_quickLink ul li a{
	padding-left:28px;
	color:#FFF;
	
}
.main_quickLink ul li img{ 
   position:absolute;
   left:20px;
   top:10px;
}

.main_link{ width:980px; margin:10px auto;}
.main_link ul li{
	width:236px;
	height:90px;
	margin-right:12px;
	float:left;
	text-align:center;
	position:relative;
	
} 
.main_link ul li img{
	position:absolute;
	left:80px;
    top:4px;
	}
.main_link ul li a{ 
	display:block;
	color:#fff;
	margin-top:60px;
	font-size:18px;
}
.main_link ul .link1{
	background: #ffd27c;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ffd27c), to(#ffba35));
	background: -webkit-linear-gradient(#ffd27c, #ffba35);
	background: -moz-linear-gradient(#ffd27c, #ffba35);
	background: -ms-linear-gradient(#ffd27c, #ffba35);
	background: -o-linear-gradient(#ffd27c, #ffba35);
	background: linear-gradient(#ffd27c, #ffba35);
	-pie-background: linear-gradient(#ffd27c, #ffba35);
	}
.main_link ul .link2{
	background: #9cc8f5;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#9cc8f5), to(#84b2e2));
	background: -webkit-linear-gradient(#9cc8f5, #84b2e2);
	background: -moz-linear-gradient(#9cc8f5, #84b2e2);
	background: -ms-linear-gradient(#9cc8f5, #84b2e2);
	background: -o-linear-gradient(#9cc8f5, #84b2e2);
	background: linear-gradient(#9cc8f5, #84b2e2);
	-pie-background: linear-gradient(#9cc8f5, #84b2e2);

}
.main_link ul .link3{
	
	background: #3dd8b9;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#3dd8b9), to(#1abc9c));
	background: -webkit-linear-gradient(#3dd8b9, #1abc9c);
	background: -moz-linear-gradient(#3dd8b9, #1abc9c);
	background: -ms-linear-gradient(#3dd8b9, #1abc9c);
	background: -o-linear-gradient(#3dd8b9, #1abc9c);
	background: linear-gradient(#3dd8b9, #1abc9c);
	-pie-background: linear-gradient(#3dd8b9, #1abc9c);
	}
.main_link ul .link4{
	background: #9cd352;
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#9cd352), to(#8cc63e));
	background: -webkit-linear-gradient(#9cd352, #8cc63e);
	background: -moz-linear-gradient(#9cd352, #8cc63e);
	background: -ms-linear-gradient(#9cd352, #8cc63e);
	background: -o-linear-gradient(#9cd352, #8cc63e);
	background: linear-gradient(#9cd352, #8cc63e);
	-pie-background: linear-gradient(#9cd352, #8cc63e);
	margin-right:0px;
	}
	
.main_link_content{width:980px; margin:0 auto 0 auto; color:#FFF;}
.main_link_content a{ color:#FFF;}
.main_link_content ul li{
	width:236px;
	height:420px;
	margin-right:12px;
	float:left;
	position:relative;
	
} 
.main_link_content ul .link_content1{ background:#ffba35;}
.main_link_content ul .link_content2{ background:#84b2e2;}
.main_link_content ul .link_content3{ background:#1abc9c;}
.main_link_content ul .link_content4{
	background:#FFF;
	margin-right:0px;
	}
.link_article{
	padding-bottom:10px;
	margin:10px 8px 5px 8px;
	border-bottom:dotted 2px #FFF;
	}
.link_article h3{
	font-weight:normal;
	font-size:16px;
	text-indent:20px;
	background:url(../images/index/point.png) no-repeat ;
	}
.link_article img{ border:1px solid #fff; margin:5px 0;}
.link_expert {
	position:relative;
	margin:5px 8px 10px 8px;
	height:138px;}
.link_expert h2{
	font-weight:normal;
	font-size:18px;
	} 
.link_expert dl .expert_img{
	position:absolute;
	left:5px;
	top:30px;}
.link_expert dl .expert_name{
	font-size:16px;
	color:#fff;
	padding-left:98px;
	}
.link_expert dl .expert_detail{
	font-size:12px;
	padding-left:98px;
	}
.link_expert dl .more_expert{
	font-size:18px;
	padding-left:98px;
	text-align:right;
	}
.main_link_content ul .link_content4 .box1{
	width:115px;
	margin-right:6px;
	float:left;
	background:#86c136;
	height:90px;
	}
.main_link_content ul .link_content4 .box2{
	width:115px;
	float:left;
	background:#86c136;
	height:90px;
	}
.main_link_content ul .link_content4 .box3{
	width:236px;
	float:left;
	background:#86c136;
	height:190px;
	margin:10px 0;
	font-size:12px;
	}
.main_link_content ul .link_content4 .box3 dl{
	margin:8px;
	
	}
.main_link_content ul .link_content4 .box3 dl dt{
	font-size:16px;
	border-bottom:dotted 2px #fff;
	position:relative;
	line-height:32px;
	}
.main_link_content ul .link_content4 .box3 dl span{
	position:absolute;
	right:2px;
	top:0px;
	
	}
.main_link_content ul .link_content4 .box3 dl dd{
	line-height:20px;
	}
.main_link_content ul .link_content4 .box4{
	width:236px;
	float:left;
	background:#9cd352;
	height:120px;
	}
.famous_expert{position:relative; margin:5px;}
.famous_expert_img{position:absolute; left:5px; top:30px;}
.famous_expert h2{ font-size:18px; font-weight:lighter;}
.famous_expert_name{ margin-left:90px;}
.famous_expert_detail{ margin-left:90px;font-size:12px; line-height:16px;}

.main_tool{ width:980px; background:#d5edb4; margin:10px auto; height:172px;}
.main_tool h3{ color:#1abc9c; font-size:18px; padding:10px 0 0 10px;}
.main_tool_link{ background:#FFF; margin:5px; clear:both;}
.main_tool_link ul li{ float:left; width:90px; height:122px; margin:0 3px; text-align:center; position:relative;}
.main_tool_link ul li img:hover{ width:90px;height:90px; cursor:pointer;}
.main_tool_link ul li div{ position:absolute;bottom:10px; left:0; width:90px; text-align:center; }

.main_map_hospital{ width:980px; margin:0 auto;}
.main_map_wrap{ width:208px; height:204px; margin-right:10px; background:#f8fcf5;border:1px solid #c3e4ad; float:left}
.main_map_wrap h3{color:#1abc9c; font-size:18px; padding:10px 0 0 10px; }
.main_hospital_wrap{ width:758px;height:204px;background:#f8fcf5; border:1px solid #c3e4ad; float:left;}

.friendLink{width:980px; margin:0 auto;}
.friendLink{color:#1abc9c; font-size:18px; padding:10px 0 0 10px;}
.friendLink ul li{ float:left;  margin:10px; font-size:14px; color:#4e4e4e;}

.bottomBak{ background:url(../images/index/bottom_bak.png) repeat-x; width:100%; height:202px; text-align:center;  color:#fff;}
.bottomBak p{ padding-top:140px;}


